{
    "name": "gildonei/nfse-nacional",
    "description": "Emissor de Nota Fiscal de Serviço Eletrônica (NFS-e) Nacional em PHP",
    "type": "library",
    "version": "1.0.1",
    "license": "MIT",
    "keywords": ["nfse", "nota-fiscal", "servico", "brasil", "receita-federal"],
    "authors": [
        {
            "name": "Gildonei M A Junior",
            "email": "gildonei.mendes@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "robrichards/xmlseclibs": "^3.1",
        "symfony/dotenv": "^7.4",
        "symfony/var-dumper": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NfseNacional\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NfseNacional\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
