{
    "name": "nfse-nacional/nfse-nacional",
    "description": "Pacote PHP para integração com a API NFS-e do Governo Federal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gildonei M A Junior",
            "email": "gildonei.mendes@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/options-resolver": "^6.0|^7.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "robrichards/xmlseclibs": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NfseNacional\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NfseNacional\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

